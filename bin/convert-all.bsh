#!/bin/bash

cd ~/git/eopaod/src

function convert_country {
  echo "Converting $1..."
  ruby psv2csv.rb $1
  ruby psv2svg.rb $1
}

convert_country al
convert_country be-bru
convert_country be-vlg
convert_country be-wal
convert_country cy
convert_country fr
convert_country gb-nir
convert_country ge
convert_country ie
convert_country ru
